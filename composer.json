{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-mongodb": "*",
        "ext-xml": "*",
        "ext-json": "*",
        "alcaeus/mongo-php-adapter": "^1.1",
        "cakephp/chronos": "^1.2",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/mongodb-odm": "^1.2",
        "doctrine/mongodb-odm-bundle": "^3.5",
        "doctrine/orm": "^2.6",
        "lexik/jwt-authentication-bundle": "^2.6",
        "prooph/event-sourcing": "^5.6",
        "prooph/event-store-bus-bridge": "^3.3",
        "prooph/event-store-bus-bridge-symfony-bundle": "dev-master",
        "prooph/event-store-symfony-bundle": "^0.5.0",
        "prooph/pdo-event-store": "^1.10",
        "prooph/service-bus": "^6.2",
        "prooph/service-bus-symfony-bundle": "^0.7.0",
        "respect/validation": "^1.1",
        "symfony/console": "4.2.*",
        "symfony/flex": "^1.2",
        "symfony/framework-bundle": "4.2.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/twig-bundle": "4.2.*",
        "symfony/yaml": "4.2.*"
    },
    "require-dev": {
        "symfony/dotenv": "4.2.*",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "ext-mongo": "1.6.16"
        }
    },
    "autoload": {
        "psr-4": {
            "WalletAccountant\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WalletAccountant\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
